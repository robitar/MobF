    "test": "tests"
  },
  "scripts": {
    "build": "dotnet build ./MobF.sln",
    "test": "dotnet fable tests --exclude MobF.ObserverPlugin --configuration Debug -s --noCache --outDir dist/tests && mocha dist/tests"
  },
  "repository": {
    "type": "git",
