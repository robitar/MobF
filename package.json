  "license": "MIT",
  "bugs": {
    "url": "https://github.com/robitar/MobF/issues"
  },
  "homepage": "https://github.com/robitar/MobF#readme",
  "dependencies": {
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.0.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1"
  },
  "devDependencies": {
    "@testing-library/react": "^16.0.1",
    "global-jsdom": "^25.0.0",
    "jsdom": "^25.0.1",
    "mocha": "^11.0.1"
  }
